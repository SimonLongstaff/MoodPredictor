<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/moodpredictTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Current_Mood"
        android:layout_marginTop="40dp"
        android:layout_gravity="center_horizontal"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Current_Mood_Prediction"
        android:layout_marginTop="60dp"
        android:layout_gravity="center_horizontal"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/steps_today"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="200dp"
        />

    <TextView
        android:id="@+id/numSteps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="220dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/screen_on_time"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="400dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/screenTime"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="420dp"
        />




</FrameLayout>